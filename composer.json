{
    "name": "ghostcodelover/user-bundle",
    "type": "symfony-bundle",
    "description": "Symfony ZNDUserBundle",
    "keywords": ["User management"],
    "homepage": "http://zandu.cd",
    "license": "MIT",
    "authors": [
        {
            "name": "Mukendi Emmanuel",
            "email": "mukendiemmanuel15@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.1",
        "symfony/security-bundle": "~2.1"
    },
    "require-dev": {
        "twig/twig": "~1.5",
        "doctrine/doctrine-bundle": "~1.3",
        "swiftmailer/swiftmailer": "~4.3|~5",
        "willdurand/propel-typehintable-behavior": "~1.0",
        "symfony/yaml": "~2.1",
        "symfony/validator": "~2.1"
    },
    "suggest": {
        "willdurand/propel-typehintable-behavior": "Needed when using the propel implementation"
    },
    "autoload": {
        "psr-0": { "ZND\\USM\\UserBundle": "" }
    },
    "target-dir": "ZND/USM/UserBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
